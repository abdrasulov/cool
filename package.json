{
  "name": "mdm-server",
  "version": "1.0.0",
  "description": "Simple MDM server for iPhone management",
  "private": true,
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npx react-scripts start",
    "build": "cd client && npx react-scripts build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "plist": "^3.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
